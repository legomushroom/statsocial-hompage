@import '../assets/imports.styl'

.pages-btns
	position absolute
	right 20px
	bottom 20px
	transform translateY(-50%)
	z-index 11
	&__play
		circle 4*gs
		background-color white
		position absolute
		top  - 1.5*gs
		left - 5*gs
		cursor pointer
		.at-mobiles &
			top - gs
		.icon
			rect 1.5*gs
			fill #ff6c66
		&__icon
			position absolute
			nw 50%
			margin-left - .75*gs
			margin-top  - .75*gs
			fill #777
		&__play
			margin-left - .6*gs
		&__pause
			display none
		&.is-playing
			.pages-btns__play__pause
				display block
			.pages-btns__play__play
				display none
			
	&.is-transparent
		opacity .75
		.at-mobiles &
			opacity 1
		&:hover
			opacity 1
	&__btn
