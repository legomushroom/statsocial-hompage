        .menu-suggest#js-menu-suggest menu

        .main-header.is-hidden#js-main-menu
          .sidebar-logo-e.is-blue-bg
              | STAT SOCIAL
            .page-menu-header#js-menu-btn
              .icon.page-header2--menu-icon(title='')
