!!!5
html
	head
		meta(charset='UTF-8')
		title doc
		link(href="css/main.css", type="text/css", rel="stylesheet")
		link(href='http://fonts.googleapis.com/css?family=Open+Sans:400italic,400,300,600,700,800', rel='stylesheet', type='text/css')
	
	//- body.no-amimation-g
	body
		- var el = { }
		include css/icons.svg
		section(role="main")#js-main.main-l
			#scroller
				.curtain-l#js-curtain1
					.script-l.is-1#js-script1 behind all this clutter of social data

					.script-l.is-status-quo#js-main-logo Status Quo
					.layer(data-depth="0.10")
						.image1.curtain-layer-lh
					.layer(data-depth="0.15")
						.image2.curtain-layer-lh
					.layer(data-depth="0.20")
						.image3.curtain-layer-lh
					.layer(data-depth="0.50")
						.image4.curtain-layer-lh
					.layer(data-depth="0.60")
						.image5.curtain-layer-lh
					.layer(data-depth="0.85")
						.image6.curtain-layer-lh
					.layer(data-depth="1.00")
						.image7.curtain-layer-lh

				.curtain2-l#js-curtain2
					.script-l.is-2#js-script2 lies a world of knowledge and clarity
					.curtain2-wrapper-lh
						.curtain2-body-lh
							.curtain2-mask-lh
							- var forkSize = 12
							.curtain2-left-lh#js-curtain2-left-side
								.curtain2-konfetti-lh
									.layer(data-depth="0.1")
										.image1.curtain-layer-lh
									.layer(data-depth="0.1")
										.image2.curtain-layer-lh
									.layer(data-depth="0.4")
										.image3.curtain-layer-lh
									.layer(data-depth="0.5")
										.image4.curtain-layer-lh
									.layer(data-depth="0.4")
										.image5.curtain-layer-lh
									.layer(data-depth="0.5")
										.image6.curtain-layer-lh

								- for (var i=0; i< forkSize; i++)
									.curtain2-section-lh.is-even
										.curtain2-section-inner-lh
									.curtain2-section-lh

							.curtain2-right-lh#js-curtain2-right-side
								.curtain2-konfetti-lh
									.layer(data-depth="0.1")
										.image1.curtain-layer-lh
									.layer(data-depth="0.1")
										.image2.curtain-layer-lh
									.layer(data-depth="0.4")
										.image3.curtain-layer-lh
									.layer(data-depth="0.5")
										.image4.curtain-layer-lh
									.layer(data-depth="0.4")
										.image5.curtain-layer-lh
									.layer(data-depth="0.5")
										.image6.curtain-layer-lh

								- for (var i=0; i< forkSize; i++)
									.curtain2-section-lh
									.curtain2-section-lh.is-even
										.curtain2-section-inner-lh

				.curtain3-l#js-curtain3
					.bg-ll#js-bg
					.ground-ll#js-ground
					.curtain3-cloud-holder-lh.curtain3-cloud-holder1-lh
						- el = { class: 'curtain3-cloud1-lh', id: '' }
						include css/partials/cloud
					
					.curtain3-cloud-holder-lh.curtain3-cloud-holder2-lh
						- el = { class: 'curtain3-cloud2-lh cloud2-b', id: '' }
						include css/partials/cloud

					.curtain3-cloud-holder-lh.curtain3-cloud-holder3-lh
						- el = { class: 'curtain3-cloud3-lh cloud2-b', id: '' }
						include css/partials/cloud

					.curtain3-cloud-holder-lh.curtain3-cloud-holder4-lh
						- el = { class: 'curtain3-cloud4-lh', id: '' }
						include css/partials/cloud

					.curtain3-cloud-holder-lh.curtain3-cloud-holder5-lh
						- el = { class: 'curtain3-cloud5-lh cloud2-b', id: '' }
						include css/partials/cloud

					- var buildings = ['location', '23%', '44%', 'education', '12%', '6%', 'age', '14%', '6%', 'profession', '10%', '5%', 'sex']
					- for (var i = 1; i<=buildings.length; i++)
						- el = { text: buildings[i-1], class: 'curtain3-building'+i+'-lh', id: '' }
						include css/partials/building

					.curtain3-plane-lh#js-plane
						- el = { class: '', id: 'js-plane-inner', text: 'detailed statistics over many fine segments' }
						include css/partials/plane-n-text


					- el = { class: 'curtain3--bush-lh curtain3--bush1-lh ', id: '' }
					include css/partials/bush

					- el = { class: 'curtain3--bush-lh curtain3--bush2-lh is-light is-flip', id: '' }
					include css/partials/bush

					- el = { class: 'curtain3--bush-lh curtain3--bush3-lh is-light is-flip', id: '' }
					include css/partials/bush

					- el = { class: 'curtain3--bush-lh curtain3--bush4-lh ', id: '' }
					include css/partials/bush

					- el = { class: 'curtain3--bush-lh curtain3--bush5-lh ', id: '' }
					include css/partials/bush

					- el = { class: 'curtain3--bush-lh curtain3--bush6-lh is-light is-flip', id: '' }
					include css/partials/bush

					- el = { class: 'curtain3--bush-lh curtain3--bush7-lh is-light is-flip', id: '' }
					include css/partials/bush

					- el = { class: 'curtain3--bush-lh curtain3--bush8-lh is-flip is-light', id: '' }
					include css/partials/bush

					- el = { class: 'curtain3--bush-lh curtain3--bush9-lh is-flip is-light', id: '' }
					include css/partials/bush

					- el = { class: 'curtain3--bush-lh curtain3--bush10-lh', id: '' }
					include css/partials/bush

					- el = { class: 'curtain3--bush-lh curtain3--bush11-lh', id: '' }
					include css/partials/bush

					- el = { class: 'curtain3--bush-lh curtain3--bush12-lh is-flip', id: '' }
					include css/partials/bush

					.roller-coaster-b
						svg(x='0px', y='0px', width='1220px', height='504px', viewbox='0 0 1220 504', enable-background='new 0 0 1220 504', xml:space='preserve')
							marker(id="marker-circle", viewBox="0 0 16 16", refX="8", refY="8", markerUnits="strokeWidth", markerWidth="7", markerHeight="7", fill="#409e9c", orient="auto")
								circle(cx="8", cy="8", r="5", stroke="#257978", stroke-width="2")

							marker(id="marker-arrow", viewBox="0 0 22 42", refX="-22", refY="5", markerUnits="strokeWidth", markerWidth="42", markerHeight="12", fill="#409e9c", orient="auto")
								polygon(transform="rotate(90)", x="4", fill="#287E7C", points="3.833,0 0,41 9.833,41")

							marker(id="marker-arrow2", viewBox="0 0 22 42", refX="-22", refY="5", markerUnits="strokeWidth", markerWidth="42", markerHeight="12", fill="#409e9c", orient="auto")
								polygon(transform="rotate(90)", x="4", fill="#287E7C", points="3.833,0 0,41 9.833,41")
							
							line(id="roller-y", transform="translate(0,520)", marker-end="url(#marker-arrow)", fill='none', stroke-miterlimit='10', x1='7', y1='498.959', x2='7', y2='498')
							line(id="roller-x", marker-end="url(#marker-arrow)", fill='none', stroke-miterlimit='10', x1='4.933', y1='497', x2='1180', y2='497')
							polyline(id="roller-line1", marker-mid="url(#marker-circle)", fill='none', stroke-miterlimit='10', points='4.933,217.667 58.333,203 129,217.667 233.667,265.667 320.953,265.667 374.333,234.333 428.333,217 481,217 534.333,234.333 639.667,295 782.334,327 855.667,330.334 925.667,314.334 1031,294.334 1140.334,252 1223.667,229.667')
							polyline(id="roller-line2", marker-mid="url(#marker-circle)", fill='none', stroke-miterlimit='10', points='5.016,153.667 38.333,145 147.667,153.667 252.333,214.333 339.667,279.667 428.333,299 515,286.334 603,248.333 711,225.667 821,271.667 925.667,314.334 1031,294.334 1140.334,252 1223.667,229.667 ')
					

		script(src="js/jquery-2.1.0.min.js")
		script(src="js/jquery.superscrollorama.js")
		script(src="js/iscroll.js")
		script(src="js/tweenmax.min.js")
		script(src="js/jquery.parallax.js")
		script(src="js/helpers.js")
		script(src="js/main.js")










