          .baloon.baloon__green2.js-baloon__layer2.curtain3--baloon-green2

          .baloon.baloon__red1.js-baloon__layer3.curtain3--baloon-red1
          .baloon.baloon__red2.js-baloon__layer3.curtain3--baloon-red2

          - var icons = ['facebook', 'linkedin', 'google-plus', 'foursquare', 'quora', 'blogging', 'youtube', 'myspace' ]
          .ferris-wheel#js-ferris-wheel
            svg.ferris-wheel--script-path(width='731px', height='560px', viewbox='0 0 731 560')
              defs
                path(id="ferris-script", fill='none', stroke='#E30613', stroke-width='2', stroke-miterlimit='10', d='M-522.865,1384.396L57.03,347.583c0,0,67.789-114.568,90.385-152.757l0,0C187.87,123.386,264.57,75.175,352.53,75.175c130.094,0,235.555,105.461,235.555,235.556c0,105.606-69.498,194.983-165.256,224.886L-721.942,922.059')
              text
                textPath(id="js-ferris-text", xlink:href="#ferris-script", startOffset="0") data harvested from 60+ social networks
            
            .ferris-wheel--core
              - for (var i=0; i<12; i++)
                - el = { class: 'n-'+i + ' n-'+icons[ i % icons.length ], id: '' }
                - icon = { path: icons[ i % icons.length ] + '-icon'  }
                include css/partials/cabin
                  - for (var j=0; j<random(2,3); j++)
                    - el = { class: 'human-'+j, id: '' }
                    include css/partials/human

              .ferris-wheel--circle1
              .ferris-wheel--circle2

              - var spikesCnt = 24
              - for (var i=0; i<spikesCnt; i++)
                .ferris-wheel--spike(class="n-#{i}")

              - for (var i=0; i<spikesCnt; i++)
                .ferris-wheel--spike(class="s-#{i}")

            .ferris-base
              .ferris-base--center
                - el = { path: 'twitter-icon'  }
                include css/partials/icon
                
              .ferris-base--top
              .ferris-base--main
              .ferris-base--spike.is-1
              .ferris-base--spike.is-2
              .ferris-base--handle


          .moon-n-text#js-moon
            .moon-n-text--side.moon-n-text--side__left Male
              div 50%
            .moon-n-text--side.moon-n-text--side__right Female
              div 50%
            - el = { class: '', id: ''} 
            include css/partials/moon

          .entrance#js-entrance

          .carousel#js-carousel
            .carousel-top
              .carousel-top--circle-large
              .carousel-top--circle-small

            .carousel-bottom
              .carousel-bottom--top
              .carousel-bottom--bottom
              .carousel-bottom--bottom2
              .carousel-bottom--bottom3

            .carousel--3d
              .carousel--scene
                .carousel--stage
                  .model
                    - var width = 50, height = 370.5, amount = 36;
                    - var em = 1/16;
                    - for (var i=1; i<=amount;i++)  
                      .dome(class="s#{i}")
                        svg(style='width: #{width*em}em; height: #{height*em}em;')
                          polygon(points='#{width/2},0 #{width},#{height} 0,#{height}')

                .scene
                  .carousel-middle

                  .shape.cylinder-1.cyl-1
                    - var captions = ['Brands', 'Interests', 'Celebrities', 'TV shows']
                    - var c = -1;
                    - for(var i=0; i<=36;i++)
                      .face.side(class="s#{i}")
                        if (i % 4 == 0)
                          c = c + 1
                          text = (c < 9) ? captions[c%captions.length] : captions[3]
                          - el = { text: text, class: 'is-'+(text.toLowerCase().replace(/\s/gi, '\-')) }
                          include css/partials/banner



                  - icons = ['fashion', 'education', 'plant', 'book', 'bowling', 'tv', 'celebrities', 'joystick2']
                  - r = -1
                  - for(var i=2; i<=9;i++)
                    .shape.cylinder-2(class="cyl-#{i}")
                      - for(var j=0; j<=2;j++)
                        .face.side(class="s#{j}")
                          if (j == 2)
                            r = r+1
                            - el = { }
                            - icon = { path: icons[r]+'-icon' }
                            include css/partials/icon-banner

